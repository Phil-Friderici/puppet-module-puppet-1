{
  "name": "gillarkod-puppet",
  "version": "0.2.0",
  "author": "propyless",
  "summary": "Manage Puppet",
  "description": "Manages Puppet Agent and Master",
  "license": "Apache-2.0",
  "source": "git://github.com/gillarkod/puppet-module-puppet.git",
  "project_page": "https://github.com/gillarkod/puppet-module-puppet",
  "issues_url": "https://github.com/gillarkod/puppet-module-puppet/issues",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.3.1 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "SLES"
    },
    {
      "operatingsystem": "SLED"
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04"
      ]
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.6.0 < 6.0.0"},
    {"name":"puppetlabs/inifile","version_requirement":">= 1.5.0"},
    {"name":"puppetlabs/hocon","version_requirement":">= 0.9.0 < 0.9.5"},
    {"name":"puppetlabs/puppet_authorization","version_requirement":">= 0.2.0"}
  ]
}

